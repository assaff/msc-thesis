    pages = {3140--3149},
    posted-at = {2011-04-12 15:51:29},
    priority = {2},
    publisher = {Wiley Subscription Services, Inc., A Wiley Company},
    title = {Can self-inhibitory peptides be derived from the interfaces of globular protein–protein interactions?},
