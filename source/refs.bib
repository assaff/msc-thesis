    citeulike-linkout-3 = {http://www3.interscience.wiley.com/cgi-bin/abstract/123324632/ABSTRACT},
    doi = {10.1002/prot.22716},
    issn = {1097-0134},
    journal = {Proteins},
    keywords = {interaction, modeling, peptide, protein, raveh10pdf, rosetta},
    month = jul,
    number = {9},
